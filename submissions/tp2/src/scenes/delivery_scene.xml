        <primitive id="pole">
            <cylinder2 base="0.02" top="0.02" height="2.5" slices="10" stacks="4" />
        </primitive>
        <primitive id="banner_holder">
            <patch npointsU="3" npointsV="2" npartsU="15" npartsV="2">
                <controlpoint xx="-1" yy="1.75" zz="0" />
