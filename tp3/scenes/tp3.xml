<yas>
    <scene root="root_comp" axis_length="8" />
    <views default="camera" >
        <perspective id="camera" near="0.1" far="90" angle="60">
            <from x="10" y="5" z="12" />
            <to x="0" y="0" z="0" />
        </perspective>
    </views>
    <ambient>
        <ambient r="0.6" g="0.6" b="0.6" a="1" />
        <background r="0.05" g="0.05" b="0.05" a="1" />
    </ambient>
    <lights>
        <omni id="room_top1" enabled="t" >
            <location x="0" y="3.5" z="0" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
        <omni id="room_top2" enabled="t" >
            <location x="8" y="3.5" z="0" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
        <omni id="room_top3" enabled="t" >
            <location x="0" y="3.5" z="8" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
        <omni id="room_top4" enabled="t" >
            <location x="8" y="3.5" z="8" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
    </lights>
    <textures>
        <texture id="tex_stark" file="scenes/images/stark.jpg" />
    </textures>
    <materials>
        <material id="mat_regular" shininess="40" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0.3" g="0.3" b="0.3" a="1" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </material>
    </materials>
    <transformations>
        <transformation id="trf">
            <rotate axis="x" angle="0" />
        </transformation>
    </transformations>
    <animations>
    </animations>
    <primitives>
        <primitive id="board">
            <board />
        </primitive>
    </primitives>
    <components>
        <component id="root_comp">
            <transformation>
            </transformation>
            <animations></animations>
            <materials>
                <material id="mat_regular" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="board_comp" />
            </children>
        </component>

        <component id="board_comp">
            <transformation>
            </transformation>
            <animations></animations>
            <materials>
                <material id="mat_regular" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <primitiveref id="board" />
            </children>
        </component>

    </components>
</yas>